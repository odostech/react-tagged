{
  "name": "react-tagged",
  "module": "dist/react-tagged.mjs",
  "main": "dist/react-tagged.umd.js",
  "exports": {
    ".": {
      "import": "./dist/react-tagged.mjs",
      "require": "./dist/react-tagged.umd.js"
    }
  },
  "types": "dist/index.d.ts",
  "version": "1.1.0",
  "description": "A robust tags input for React with TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/odostech/react-tagged.git"
  },
  "scripts": {
    "test": "vitest --run",
    "test:watch": "jest --watch",
    "build": "rm -rf dist && vite build",
    "examples": "rollup --config rollup.config.examples.js --watch",
    "types": "tsc --project tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "react": ">= 16.8"
  },
  "keywords": [
    "react",
    "tags"
  ],
  "author": "weareodos",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "happy-dom": "^15.7.4",
    "react": ">= 16.8",
    "react-dom": ">= 16.8",
    "sass": "^1.37.5",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.1"
  }
}
